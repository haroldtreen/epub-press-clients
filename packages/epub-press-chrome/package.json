{
  "description": "Chrome extension for epub-press",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "export ENV=test && open http://localhost:5001/tests && webpack-dev-server",
    "build": "export ENV=development && webpack",
    "build-prod": "export NODE_ENV=production && webpack --optimize-minimize --optimize-dedupe",
    "start": "export ENV=development && webpack --watch --color --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haroldtreen/epub-press-clients.git"
  },
  "author": "EpubPress",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/haroldtreen/epub-press-clients/issues"
  },
  "homepage": "https://github.com/haroldtreen/epub-press-clients#readme",
  "dependencies": {
    "bluebird": "^3.4.1",
    "epub-press-js": "^0.5.0",
    "jquery": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "fetch-mock": "^5.1.1",
    "mocha": "^3.0.2",
    "mocha-loader": "^0.7.1",
    "webpack": "^1.13.1"
  }
}
